{{>layouts/header}}
<table class="table">
    <thead>
    <tr>
        <th scope="col">id</th>
        <th scope="col">pwd</th>
        <th scope="col">name</th>
        <th scope="col">email</th>
        <th scope="col">joindate</th>
        <td>삭제</td>
    </tr>
    </thead>
    <tbody>
    {{#memberList}}
        <tr>
            <th>{{id}}</th>
            <td>{{pwd}}</td>
            <td><a href="/members/{{id}}/edit">{{name}}</a></td>
            <td>{{email}}</td>
            <td>{{joindate}}</td>
            <td><a href="/members/{{id}}/delete">삭제</a></td>
        </tr>
    {{/memberList}}
    </tbody>
</table>
<a href="/members/new">New Member</a>
{{>layouts/footer}}